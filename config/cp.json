{
  "dns": {
    "app": {
      "filename": "appqueries.json",
      "frequencySec": 3,
      "nameserver": "127.0.0.1"
    },
    "cache": {
      "concurrent": 25,
      "intervalSec": 60,
      "percentage": 80,
      "queryTimeoutMilliSec": 1000,
      "queryTries": 1
    },
    "ops": {
      "filename": "opsqueries.json",
      "frequencySec": 3,
      "nameserver": "127.0.0.1"
    }
  },
  "global": {
    "gxConfig": 0,
    "loggingLevel": "LOG_DEBUG",
    "periodicTimerSec": 10,
    "requestTimeoutMilliSec": 5000,
    "requestTries": 3,
    "transmitCount": 5,
    "transmitTimerSec": 2,
    "urrConfig": 0
  },
  "ip_pool_config": {
    "staticUeIpPool": {
      "ip": "10.249.1.0",
      "mask": "255.255.255.0"
    },
    "ueIpPool": {
      "ip": "10.250.0.0",
      "mask": "255.255.0.0"
    }
  }
}
